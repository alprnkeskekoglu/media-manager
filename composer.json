{
    "name": "dawnstar/media-manager",
    "description": "Package for Media Manager",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "intervention/image": "^2.7",
    },
    "autoload": {
        "psr-4": {
            "Dawnstar\\MediaManager\\": "src"
        },
        "files": [
            "src\\Http\\helpers.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "classmap-authoritative": true,
        "apcu-autoloader": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Dawnstar\\MediaManager\\MediaManagerServiceProvider"
            ]
        }
    }
}

